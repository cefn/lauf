{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "jsx": "react",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "noFallthroughCasesInSwitch": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "composite": true
    },
    "include": [
        "src/**/*",
        "test/**/*"
    ],
    "references": []
}